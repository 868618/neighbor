.navbar{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  box-sizing: border-box;
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
  z-index: 1000;
}
.navbar_box{
  display: flex;
  align-items: center;
  width: 100%;
  box-sizing: border-box;
  /*background-color: green;*/
}

.navbar_location .title{
  position: relative;
}

.navbar_location .title::after{
  content: '';
  position: absolute;
  right: -30rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 20rpx;
  height: 10rpx;
  background: url("https://www.juelie.net/nb/image/2020/11/17/ffdb1326712d4ed3bb326049f2d21f3e.png") no-repeat center;
  background-size: 100%;
}



.navbar .navbar_custom{
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.navbar_box .icon{
  width: 26rpx;
  height: 32rpx;
  margin-right: 6rpx;
}
.back_arrow{
  width: 18rpx !important;
  display: inline-block;
  /*padding: 0rpx 20rpx;*/
  /*padding-left: 0;*/
  padding-right: 20rpx;
}
.location_icon{
  margin-left: 10rpx;
}

.navbar_box .title{
  font-size: 32rpx;
  font-weight: 400;
  color: #353535;
  margin-left: 10rpx;
}


.bastard{
  opacity: 0;
}