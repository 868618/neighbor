.navbar{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  box-sizing: border-box;
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
  z-index: 1000;
}
.navbar_box{
  display: flex;
  align-items: center;
  width: 100%;
}
