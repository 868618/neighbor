/* components/Tabbar/Tabbar.wxss */
.tabbar{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 111rpx;
  z-index: 99999;
}


.tabbar .active{
  color: #FE990C;
}

.tabbar .tabbar_item{
  width: 50%;
  height: 98rpx;
  line-height: 98rpx;
  display: inline-block;
  margin-top: 13rpx;
  box-sizing: border-box;
  background: #FAFAFA;
  border-top: 1rpx solid rgba(0,0,0,.25);
}

.tabbar .tabbar_item  .tabbar_item_box{
  text-align: center;
  display: flex;
  font-size: 20rpx;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.tabbar .tabbar_item:first-of-type .tabbar_item_box{
  float: left;
}

.tabbar .tabbar_item:last-of-type .tabbar_item_box{
  float: right;
}

.tabbar .tabbar_item .tabbar_item_box cover-image {
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 10rpx;
}

.tabbar .tabbar_left{
  text-align: left;
  padding-left: 120rpx;
}

.tabbar .tabbar_right{
  text-align: right;
  padding-right: 120rpx;
}

.tabbar_center{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 0;
  width: 111rpx;
  height: 111rpx;
  z-index: 888;
}

.tabbar_center .tabbar_center_box {
  width: 100%;
  height: 100%;
  background: #FE990C;
  border-radius: 50%;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  font-size: 20rpx;
  color:rgba(255,255,255,1);
}

.tabbar_center .tabbar_center_box  cover-image {
  width: 37rpx;
  height: 49rpx;
  margin-bottom: 10rpx;
}